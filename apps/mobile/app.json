{
  "expo": {
    "name": "KlasseChatten",
    "slug": "klassechatten",
    "version": "1.0.0",
    "scheme": "klassechatten",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.klassechatten.app"
    },
    "android": {
      "package": "com.klassechatten.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
